{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "gvieira18/sycorax",
    "type": "project",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "filament/filament": "^4.0.3",
        "filament/spatie-laravel-media-library-plugin": "^4.0.3",
        "laravel/framework": "^12.25.0",
        "laravel/tinker": "^2.10.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16.0",
        "barryvdh/laravel-ide-helper": "^3.6.0",
        "driftingly/rector-laravel": "^2.0.6",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.6.0",
        "laravel/pail": "^1.2.3",
        "laravel/pint": "^1.24.0",
        "laravel/sail": "^1.44.0",
        "laravel/telescope": "^5.11.2",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "pestphp/pest": "^3.8.3",
        "pestphp/pest-plugin-faker": "^3.0.0",
        "pestphp/pest-plugin-laravel": "^3.2.0",
        "pestphp/pest-plugin-livewire": "^3.0.0",
        "rector/rector": "^2.1.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate -q",
            "@php artisan ide-helper:meta -q"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "pint": "vendor/bin/pint --parallel",
        "test:pint": "vendor/bin/pint --test --parallel",
        "rector": "vendor/bin/rector",
        "test:rector": "vendor/bin/rector --dry-run",
        "phpstan": "vendor/bin/phpstan analyse --ansi",
        "test:phpstan": "vendor/bin/phpstan analyse --ansi",
        "test:unit": "vendor/bin/pest --parallel --compact --group=unit",
        "test:feature": "vendor/bin/pest --parallel --compact --group=feature",
        "test:cov": "vendor/bin/pest --parallel --compact --coverage",
        "check": [
            "@test:rector",
            "@test:pint",
            "@test:phpstan"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "vendor/bin/pest --parallel --compact"
        ],
        "bump:minor": [
            "composer update",
            "composer bump"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
