{
    "name": "@gvieira18/sycorax",
    "license": "MIT",
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "format": "npx prettier --write \"**/*.{json,yml,md,css}\"",
        "prepare": "npx -y husky"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "husky": "9.1.7",
        "laravel-vite-plugin": "^1.2.0",
        "lint-staged": "16.1.2",
        "prettier": "3.6.2",
        "tailwindcss": "^4.0.0",
        "vite": "^6.2.4"
    },
    "lint-staged": {
        "*.{json,yml,md,css}": [
            "prettier --write"
        ],
        "*.php": [
            "vendor/bin/pint",
            "vendor/bin/rector process"
        ]
    }
}
